    "@types/jest": "^26.0.23",
    "css-loader": "^5.2.4",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.3.1",
    "inquirer": "^8.0.0",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.45.1",
    "style-loader": "^2.0.0",
